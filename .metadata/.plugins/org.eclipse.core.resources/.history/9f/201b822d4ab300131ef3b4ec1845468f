package wholegenome.nonoverlappingbasepairs.usingslidingwindow;


public class TotalforDebug {
	
	long numberofAllBases;

	public long getNumberofAllBases() {
		return numberofAllBases;
	}

	public void setNumberofAllBases(long numberofAllBases) {
		this.numberofAllBases = numberofAllBases;
	}

	public TotalforDebug() {
		super();
		this.numberofAllBases = Commons.LONG_ZERO;
	}
	
	
	

}
