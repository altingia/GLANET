package intervaltree;

/**
 * 
 */
public class DnaseIntervalTreeNodeWithNumbers extends IntervalTreeNode{
	
	//Just for search ENCODE Dnase output
	short cellLineName;
	short fileName;
	
	
	public short getCellLineName() {
		return cellLineName;
	}
	public void setCellLineName(short cellLineName) {
		this.cellLineName = cellLineName;
	}
	public short getFileName() {
		return fileName;
	}
	public void setFileName(short fileName) {
		this.fileName = fileName;
	}
	
	
	//For Encode dnase
	public DnaseIntervalTreeNodeWithNumbers(ChromosomeName chromName,int low, int high,short cellLineName, short fileName,NodeType nodeType) {
		super(chromName,low,high,nodeType);
		
		this.cellLineName = cellLineName;
		this.fileName = fileName;
		
	}	

	
	


}
