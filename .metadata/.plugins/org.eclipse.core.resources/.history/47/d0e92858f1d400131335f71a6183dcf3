package intervaltree;


/**
 * 
 */
public enum NodeType {
	
	ORIGINAL(1),
	MERGED(2);
	
	private final byte nodeType;
	
	 // Constructor
	NodeType(byte nodeType) {
        this.nodeType = nodeType;
     }
	
	 public byte nodeType()           { return nodeType; }
	   

}


