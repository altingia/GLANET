package intervaltree;

/**
 * 
 */
public class TforHistoneIntervalTreeNodeWithNumbers extends DnaseIntervalTreeNodeWithNumbers{
	
	//	Just for search ENCODE Tfbs  and Histone output
	short tfbsorHistoneName;

	public short getTfbsorHistoneName() {
		return tfbsorHistoneName;
	}

	public void setTfbsorHistoneName(short tfbsorHistoneName) {
		this.tfbsorHistoneName = tfbsorHistoneName;
	}

	public TforHistoneIntervalTreeNodeWithNumbers(ChromosomeName chromName,
			int low, int high, short cellLineName, short fileName,
			NodeType nodeType, short tfbsorHistoneName) {
		super(chromName, low, high, cellLineName, fileName, nodeType);
		this.tfbsorHistoneName = tfbsorHistoneName;
	}
	
	
	

}
